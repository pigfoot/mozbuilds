"    bzurl=http://my-project.org/bugzilla\n"
"    user=bugmail@my-project.org\n"
"    password=plugh\n"
"    version=xmlrpc\n"
"    template=Changeset {node|short} in {root|basename}.\n"
"             {hgweb}/{webroot}/rev/{node|short}\\n\n"
"             {desc}\\n\n"
"    strip=5"
msgstr ""
"    [bugzilla]\n"
"    bzurl=http://my-project.org/bugzilla\n"
"    user=bugmail@my-project.org\n"
"    password=plugh\n"
"    version=xmlrpc\n"
"    template=Changeset {node|short} in {root|basename}.\n"
"             {hgweb}/{webroot}/rev/{node|short}\\n\n"
"             {desc}\\n\n"
"    strip=5"

msgid ""
"    [web]\n"
"    baseurl=http://my-project.org/hg"
msgstr ""
"    [web]\n"
"    baseurl=http://my-project.org/hg"

