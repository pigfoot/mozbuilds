msgid ""
" @  3[tip]   7c2fd3b9020c   2009-04-27 18:04 -0500   durin42\n"
" |    Add delta\n"
" |\n"
" o  2   030b686bedc4   2009-04-27 18:04 -0500   durin42\n"
" |    Add gamma\n"
" |\n"
" o  1   c561b4e977df   2009-04-27 18:04 -0500   durin42\n"
" |    Add beta\n"
" |\n"
" o  0   d8d2fcd0e319   2009-04-27 18:04 -0500   durin42\n"
"      Add alpha"
msgstr ""
" @  3[tip]   7c2fd3b9020c   2009-04-27 18:04 -0500   durin42\n"
" |    Add delta\n"
" |\n"
