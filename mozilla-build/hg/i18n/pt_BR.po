"  [extensions]\n"
"  win32text =\n"
"  [encode]\n"
"  ** = cleverencode:\n"
"  # or ** = macencode:"
msgstr ""
"  [extensions]\n"
"  win32text =\n"
"  [encode]\n"
"  ** = cleverencode:\n"
"  # or ** = macencode:"

